{
    "compilerOptions": {
      "composite": true,
      "outDir": "./dist",
      "rootDir": ".",
      "removeComments": true,
      "declaration": true,
      "preserveConstEnums": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "module": "commonjs",
      "target": "es6",
      "types": [
        "node"
      ],
      "lib": [
        "es2015",
        "dom",
        "dom.iterable",
          ]
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }